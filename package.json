{
  "name": "webpack-build-profiler",
  "version": "0.1.0",
  "description": "A Webpack plugin for getting useful statistics of your build, with threshold and failure options",
  "main": "src/index.js",
  "scripts": {
    "transpile": "babel src --out-dir dist",
    "lint": "eslint -c ./node_modules/pipeline-validate-js/.eslintrc --parser-options=ecmaVersion:6 src/index.js",
    "build": "npm run lint && npm run transpile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thescientist13/webpack-build-profiler.git"
  },
  "keywords": [
    "Webpack",
    "profiling",
    "analysis"
  ],
  "engines": {
    "node": ">= 6.9.4"
  },
  "engineStrict": true,
  "author": "Owen Buckley <owen@analogstudios.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thescientist13/webpack-build-profiler/issues"
  },
  "homepage": "https://github.com/thescientist13/webpack-build-profiler#readme",
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.22.0",
    "eslint": "3.14.1",
    "pipeline-validate-js": "1.0.1"
  }
}